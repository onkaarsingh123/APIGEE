<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="HPX_SetConnector">
    <DisplayName>SetConnector</DisplayName>
    <Properties/>
    <AssignVariable>
        <Name>connector_domain</Name>
        <Value>onkaar2011-prod.apigee.net</Value>
		<!-- For example connector_domain will be "{org}-{env}.apigee.net". -->
    </AssignVariable>
	
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>